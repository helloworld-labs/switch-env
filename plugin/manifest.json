{
    "manifest_version": 2,
    "name": "switch evn",
    "version": "1.0.0",
    "description": "完成各个环境切换",
    "icons": {
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "img/icon19.png"
        },
        "default_title": "点击切换环境",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://yanhaijing.com/",
        "https://*.yanhaijing.com/",
        "tabs",
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
